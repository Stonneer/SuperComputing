<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.business.student.dao.StudentDao">

    <!-- 查询结果列 -->
    <sql id="base_columns">
        t_role.role_id,
        t_role.role_name,
        t_role.role_code,
        t_role.remark,
        t_role.update_time,
        t_role.create_time
    </sql>

    <!-- 分页查询 -->
    <select id="queryPage" resultType="net.lab1024.sa.admin.module.business.student.domain.vo.StudentVO">
        SELECT
        <include refid="base_columns"/>
        FROM t_role
    </select>


</mapper>
